<div data-history-node-id="325" typeof="schema:WebPage" class="node node--type-page node--view-mode-full">
  <div class="header-title">
  
    <div class="container">
      <h1 class="title"><span>
<span property="schema:name">Trade Mark Rulings</span>
</span></h1>
    </div>
  </div>
  
